proc      /proc        proc   defaults                         0 0
sysfs     /sys         sysfs  defaults                         0 0
tmpfs     /tmp         tmpfs  rw,size=100M                     0 0
/dev/sda1 /            ext3   defaults,noatime,nodiratime      0 0

